{
  "name": "echo",
  "version": "0.1.0",
  "description": "Survey AI Platform - Voice-powered surveys made simple",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"cd backend && bun run dev\" \"cd frontend && bun run dev\"",
    "dev:backend": "cd backend && bun run dev",
    "dev:frontend": "cd frontend && bun run dev",
    "build": "cd frontend && bun run build",
    "build:backend": "cd backend && bun run build",
    "build:frontend": "cd frontend && bun run build",
    "start:backend": "cd backend && bun run start",
    "start:frontend": "cd frontend && bun run start"
  },
  "keywords": [
    "survey",
    "ai",
    "voice",
    "nextjs",
    "bun"
  ],
  "author": "Echo Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
